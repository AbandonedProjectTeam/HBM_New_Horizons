{
  "schemaVersion": 1,
  "id": "hbmkotlin",
  "version": "${version}",
  "name": "HBMs Nuclear Tech - New Horizons",
  "description": "This is a port of HBM's Nuclear Tech mod from 1.7.10 to Minecraft version 1.20.1 with Fabric loader.",
  "authors": [],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/hbmkotlin/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.client.hbmkotlinClient"
    ],
    "main": [
      "com.hbmkotlin"
    ]
  },
  "mixins": [
    "hbmkotlin.mixins.json",
    {
      "config": "hbmkotlin.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
